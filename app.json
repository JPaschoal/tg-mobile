{
  "expo": {
    "name": "tg-mobile",
    "entryPoint": "./src/App.js",
    "slug": "tg-mobile",
    "version": "0.3.0",
    "orientation": "portrait",
    "icon": "./assets/splash1-quadrada.png",
    "splash": {
      "image": "./assets/splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "#A1B9F8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash1-quadrada.png",
        "backgroundColor": "#A1B9F8"
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/splash1.png"
    }
  }
}
